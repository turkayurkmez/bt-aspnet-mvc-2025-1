@model IEnumerable<Category>
<div class="list-group">

    <a asp-controller="Home" asp-action="Index" asp-route-categoryId="" class="list-group-item list-group-item-action">Tüm Kategoriler</a>
    @foreach (var item in Model)
    {
        <a  asp-controller="Home" asp-action="Index" asp-route-categoryId="@item.Id" class="list-group-item list-group-item-action">@item.Name</a>
    }
    
    
   
</div>